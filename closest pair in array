#include <bits/stdc++.h>
using namespace std;
int main() 
{
  int n,sum;
  cin>>n>>sum;
  int arr[n];
  for(int i=0;i<n;i++){
    cin>>arr[i];
  }
  pair<int, int>p;
  p={-1,-1};
  int diff=999;
  for(int i=0;i<n;i++){
    for(int j=i+1;j<n;j++){
      int c=abs(arr[i]+arr[j]-sum);
      if(c<diff){
        p={arr[i],arr[j]};
        diff=c;
      }
    }
  }
  cout<<p.first<<" "<<p.second;
  return 0;
}
