#include <iostream>
#include <bits/stdc++.h>
using namespace std;
int main() 
{
  int x,y,z;
  cout<<"Enter the size of array 1:";
  cin>>x;
  cout<<"Enter the size of array 2:";
  cin>>y;
  cout<<"Enter the size of array 3:";
  cin>>z;
  int a[x],b[y],c[z];
  cout<<"Enter the elements of array 1:"<<endl;
  for(int i=0;i<x;i++){
    cin>>a[i];
  }
  cout<<"Enter the elements of array 2:"<<endl;
  for(int i=0;i<y;i++){
    cin>>b[i];
  }
  cout<<"Enter the elements of array 3:"<<endl;
  for(int i=0;i<z;i++){
    cin>>c[i];
  }
  for(int i=0;i<x;i++){
    for(int j=0;j<y;j++){
      for(int k=0;k<z;k++){
        if(a[i]==b[j] && a[i]==c[k]){
          cout<<a[i]<<" present in all 3 arrays"<<endl;
          break;
        }
      }
    }
  }
