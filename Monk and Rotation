//rotating the array k times to the left

#include<bits/stdc++.h>
using namespace std;
int main(){
	int t;
	cin>>t;
	while(t--){
		int n,k;
		cin>>n;
		cin>>k;
		int arr[n];
		for(int i=0;i<n;i++){
			cin>>arr[i];
		}
		if(k>=n){
			while(k>=n){
				k=k%n;
			}
		}
		for(int i=n-k;i<n;i++){
			cout<<arr[i]<<" ";
		}
		for(int i=0;i<n-k;i++){
			cout<<arr[i]<<" ";
		}
		cout<<"\n";
	}
}
